{
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "/usr/bin/zsh",
      "args": ["-l", "-i"],
      "env": {
        "SHELL": "/usr/bin/zsh",
        "TERM": "xterm-256color"
      }
    },
    "bash": {
      "path": "/bin/bash",
      "args": ["-l"]
    }
  },
